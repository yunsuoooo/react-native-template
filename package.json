{
  "name": "pacee",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "eslint . --fix",
    "format:check": "eslint .",
    "android": "ENVFILE=.env react-native run-android",
    "ios": "ENVFILE=.env react-native run-ios",
    "start": "ENVFILE=.env react-native start",
    "start:dev": "ENVFILE=.env.development react-native start",
    "start:staging": "ENVFILE=.env.staging react-native start",
    "start:prod": "ENVFILE=.env.production react-native start",
    "android:dev": "ENVFILE=.env.development react-native run-android",
    "android:staging": "ENVFILE=.env.staging react-native run-android",
    "android:prod": "ENVFILE=.env.production react-native run-android",
    "ios:dev": "ENVFILE=.env.development react-native run-ios",
    "ios:staging": "ENVFILE=.env.staging react-native run-ios",
    "ios:prod": "ENVFILE=.env.production react-native run-ios",
    "test": "jest",
    "clean": "rm -rf node_modules",
    "android-clean": "cd android && ./gradlew clean",
    "ios-clean": "cd ios && pod deintegrate && rm -rf Pods Podfile.lock && rm -rf ~/Library/Caches/CocoaPods ~/.cocoapods/repos && rm -rf ~/Library/Developer/Xcode/DerivedData/* && pod setup",
    "pod-install": "cd ios && RCT_NEW_ARCH_ENABLED=0 pod install --repo-update",
    "rename": "node scripts/rename-app.js"
  },
  "dependencies": {
    "@react-native/gradle-plugin": "^0.79.2",
    "@react-navigation/bottom-tabs": "^7.3.12",
    "@react-navigation/drawer": "^7.3.11",
    "@react-navigation/native": "^7.1.8",
    "@react-navigation/native-stack": "^7.3.12",
    "@supabase/supabase-js": "^2.50.0",
    "lucide-react-native": "^0.522.0",
    "nativewind": "^4.1.23",
    "react": "19.1.0",
    "react-native": "0.80.0",
    "react-native-config": "^1.5.5",
    "react-native-css-interop": "0.1.22",
    "react-native-geolocation-service": "^5.3.1",
    "react-native-gesture-handler": "^2.26.0",
    "react-native-keyboard-controller": "^1.17.1",
    "react-native-reanimated": "^3.17.5",
    "react-native-safe-area-context": "^5.4.0",
    "react-native-screens": "^4.11.1",
    "react-native-svg": "^15.12.0",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-vector-icons": "^10.2.0",
    "react-native-webview": "^13.13.5"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "18.0.0",
    "@react-native-community/cli-platform-android": "18.0.0",
    "@react-native-community/cli-platform-ios": "18.0.0",
    "@react-native/babel-preset": "0.80.0",
    "@react-native/eslint-config": "0.80.0",
    "@react-native/metro-config": "0.80.0",
    "@react-native/typescript-config": "0.80.0",
    "@stylistic/eslint-plugin": "^5.0.0",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.1.8",
    "@types/react-native-vector-icons": "^6.4.18",
    "@types/react-test-renderer": "^19.0.0",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^9.29.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "jest": "^29.6.3",
    "react-test-renderer": "19.1.0",
    "tailwindcss": "^3.4.17",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
